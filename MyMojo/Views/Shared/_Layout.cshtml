<!DOCTYPE html>
<!--[if IE 8]> <html lang="en" class="ie8"> <![endif]-->
<!--[if IE 9]> <html lang="en" class="ie9"> <![endif]-->
<!--[if !IE]><!-->
<html lang="en">
<!--<![endif]-->
<head>
    <title>@ViewBag.Title - MyMojo.ca</title>
    <!-- Meta -->
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="Responsive HTML5 Resume/CV Template for Developers">
    <meta name="author" content="Xiaoying Riley at 3rd Wave Media">
    <link rel="shortcut icon" href="favicon.ico">
    <link href='https://fonts.googleapis.com/css?family=Roboto:400,500,400italic,300italic,300,500italic,700,700italic,900,900italic' rel='stylesheet' type='text/css'>
    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
    <title>@ViewBag.Title - MyMojo.ca</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>

    <div class="wrapper">
        <div class="sidebar-wrapper">
            <div class="profile-container">
                <img class="profile" src="~/Content/images/profile.png" alt="" />
                <h1 class="name">Chris Wallace</h1>
                <h3 class="tagline">Senior .NET Developer</h3>
            </div><!--//profile-container-->

            <div class="contact-container container-block">
                <ul class="list-unstyled contact-list">
                    <li class="resume"><i class="fa fa-file-text"></i><a href="~/Content/CV/Chris Wallace - Curriculum Vitae.pdf" target="mymojo">Curriculum Vitae</a></li>
                    <li class="email"><i class="fa fa-envelope"></i><a href="mailto: chris@mymojo.ca">chris@mymojo.ca</a></li>
                    <li class="phone"><i class="fa fa-phone"></i><a href="tel:07514816588">07514816588</a></li>
                    <li class="website"><i class="fa fa-globe"></i><a href="http://mymojo.ca">mymojo.ca</a></li>
                    <li class="address"><i class="fa fa-location-arrow"></i>London, UK</li>
                    <li class="linkedin"><i class="fa fa-linkedin"></i><a href="http://www.linkedin.com/in/chris-wallace" target="mymojo">chris-wallace</a></li>
                    <li class="github"><i class="fa fa-github"></i><a href="https://github.com/wallaceiam" target="mymojo">wallaceiam</a></li>
                    
                    @*<li class="twitter"><i class="fa fa-twitter"></i><a href="https://twitter.com/wallaceiam" target="_blank">@@wallaceiam</a></li>*@
                </ul>
            </div><!--//contact-container-->
            <div class="education-container container-block">
                <h2 class="container-block-title">Certifications</h2>
                <div class="item">
                    <h4 class="degree">Microsoft 70-480</h4>
                    <h5 class="meta">Programming in HTML5 with JavaScript and CSS3</h5>
                    <div class="time">2016</div>
                </div><!--//item-->
                <div class="item">
                    <h4 class="degree">Microsoft 70-315</h4>
                    <h5 class="meta">Developing and Implementing Web Applications with Microsoft Visual C# .NET and Microsoft Visual Studio .NET</h5>
                    <div class="time">2005</div>
                </div><!--//item-->
                <div class="item">
                    <h4 class="degree">Microsoft 70-316</h4>
                    <h5 class="meta">Developing and Implementing Windows-based Applications with Microsoft Visual C# .NET and Microsoft Visual Studio .NET</h5>
                    <div class="time">2005</div>
                </div><!--//item-->
            </div><!--//certifications-->

            <div class="education-container container-block">
                <h2 class="container-block-title">Education</h2>
                <div class="item">
                    <h4 class="degree">Bachelor of Arts, Philosophy</h4>
                    <h5 class="meta">University of Waterloo, Canada</h5>
                    <div class="time">1998 - 2001</div>
                </div><!--//item-->
                <div class="item">
                    <h4 class="degree">A-Levels in Maths, English &amp; Economics</h4>
                    <h5 class="meta">Brillantmont International College, Switzerland</h5>
                    <div class="time">1995 - 1998</div>
                </div><!--//item-->
            </div><!--//education-container-->



            <div class="interests-container container-block">
                <h2 class="container-block-title">Interests</h2>
                <ul class="list-unstyled interests-list">
                    <li>Snowboarding</li>
                    <li>Traveling</li>
                    <li>Learning to play the Guitar</li>
                </ul>
            </div><!--//interests-->

        </div><!--//sidebar-wrapper-->

        <div class="main-wrapper">

            @RenderBody()


        </div><!--//main-body-->

        <a href="javascript:" id="return-to-top"><i class="fa fa-chevron-up" aria-hidden="true"></i></a>
    </div>

    <footer class="footer">
        <div class="text-center">

            <small class="copyright">&copy; @DateTime.Now.Year - MyMojo.ca</small>
        </div><!--//container-->
    </footer><!--//footer-->

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)


    @{ if (Request.Url.Host != "localhost")
        {
            <script>
                (function (i, s, o, g, r, a, m) {
                    i['GoogleAnalyticsObject'] = r; i[r] = i[r] || function () {
                        (i[r].q = i[r].q || []).push(arguments)
                    }, i[r].l = 1 * new Date(); a = s.createElement(o),
                    m = s.getElementsByTagName(o)[0]; a.async = 1; a.src = g; m.parentNode.insertBefore(a, m)
                })(window, document, 'script', 'https://www.google-analytics.com/analytics.js', 'ga');

                ga('create', 'UA-7881966-1', 'auto');
                ga('send', 'pageview');

            </script>
        }}
</body>
</html>
